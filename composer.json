{
    "name": "jurjean/spray-serializer",
    "require": {
        "zendframework/zend-code": ">=2.3.0",
        "zendframework/zend-crypt": ">=2.3.0",
        "zendframework/zend-eventmanager": ">=2.8.0",
        "symfony/filesystem": ">=2.3.0",
        "symfony/finder": ">=2.3.0",
        "php": ">=5.5.0"
    },
    "require-dev": {
        "phpunit/phpunit": "~4.8",
        "doctrine/common": ">=2.2"
    },
    "autoload": {
        "psr-4": {
            "Spray\\Serializer\\": "src/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "Spray\\Serializer\\TestAssets\\": "test/TestAssets"
        }
    },
    "extra": {
        "branch-alias": {
            "dev-master": "2.1.x-dev"
        }
    }
}
